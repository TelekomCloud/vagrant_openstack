HOST_IP=<%= @local_ip_mngt_nw %>

FLOATING_RANGE=192.168.58.0/24
NETWORK_GATEWAY=10.10.0.1
FIXED_RANGE=10.10.0.0/24

OVS_ENABLE_TUNNELING=True
ENABLE_TENANT_TUNNELS=True
TENANT_TUNNEL_RANGES=1:1000
OVS_VLAN_RANGES=
PUBLIC_INTERFACE=eth3

MULTI_HOST=1
ADMIN_PASSWORD=ppp
MYSQL_PASSWORD=ppp
RABBIT_PASSWORD=ppp
SERVICE_PASSWORD=ppp
SERVICE_TOKEN=xyzpdqlazydog

LOG=True
SYSLOG=True
LOGFILE=<%= @devstack_dir %>/logs/stack.sh.log
SCREEN_LOGDIR=<%= @devstack_dir %>/screenlogs

DATA_DIR=<%= @openstack_data_dir %>
SWIFT_DATA_DIR=<%= @openstack_data_dir %>

# disable
disable_service n-cpu
disable_service n-net
disable_service tempest

# heat
enable_service heat h-api h-api-cfn h-api-cw h-eng

# quantum
enable_service neutron q-svc
Q_USE_SECGROUP=True

# cinder
VOLUME_GROUP=cinder-volumes

# repo
NOVA_BRANCH=<%= @openstack_branch %>
CINDER_BRANCH=<%= @openstack_branch %>
GLANCE_BRANCH=<%= @openstack_branch %>
KEYSTONE_BRANCH=<%= @openstack_branch %>
QUANTUM_BRANCH=<%= @openstack_branch %>
HORIZON_BRANCH=<%= @openstack_branch %>

DEFAULT_IMAGE_NAME=<%= @devstack_img_name %>
IMAGE_URLS=<%= @devstack_img_urls %>